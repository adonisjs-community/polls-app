{
  "name": "polls-designed-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node build/server.js",
    "gcp-build": "npm run build",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@adonisjs/assembler": "6.1.3-29",
    "@adonisjs/eslint-config": "^1.2.0",
    "@adonisjs/prettier-config": "^1.2.0",
    "@adonisjs/tsconfig": "^1.2.0",
    "@alpinejs/focus": "^3.13.0",
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@japa/browser-client": "2.0.0-4",
    "@japa/plugin-adonisjs": "^2.0.1",
    "@japa/runner": "3.0.0-9",
    "@swc/core": "^1.3.100",
    "@symfony/webpack-encore": "^4.4.0",
    "@types/luxon": "^3.3.5",
    "alpine-teleport": "^1.0.1",
    "alpinejs": "^3.13.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.54.0",
    "pino-pretty": "^10.2.3",
    "playwright": "^1.37.1",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unpoly": "^2.7.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@adonisjs/attachment-lite": "^1.0.7",
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "6.1.5-33",
    "@adonisjs/cors": "^2.1.0",
    "@adonisjs/drive-s3": "^1.3.3",
    "@adonisjs/lucid": "19.0.0-5",
    "@adonisjs/lucid-slugify": "^2.2.1",
    "@adonisjs/session": "7.0.0-14",
    "@adonisjs/shield": "8.0.0-9",
    "@adonisjs/static": "^1.0.3",
    "@adonisjs/validator": "13.0.2-3",
    "argon2": "^0.31.2",
    "edge.js": "6.0.0-10",
    "luxon": "^3.4.2",
    "mime-types": "^2.1.34",
    "pg": "^8.11.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "s3-sync-client": "^4.3.1"
  },
  "type": "module",
  "eslintConfig": {
    "extends": [
      "@adonisjs/eslint-config/app"
    ],
    "rules": {
      "@typescript-eslint/explicit-member-accessibility": "off",
      "unicorn/filename-case": "off",
      "@typescript-eslint/no-shadow": "off"
    }
  },
  "prettier": "@adonisjs/prettier-config",
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#app/*": "./app/*.js",
    "#database/*": "./database/*.js",
    "#contracts/*": "./contracts/*.js"
  }
}